  "configuration": {
    "general":
    {
      "title": "Funktionen",
      "fields": {
        "buyButtonSwitchActive": {
          "type": "checkbox",
          "label": "Shop automatisch online und offline schalten",
          "tooltip": {
            "type": "info",
            "text": "Es wird die Preisanzeige deaktiviert, damit der Kunde nicht bestellen kann. Pop-up Texte bleiben unverändert und ein komplettes deaktivieren des Shops findet nicht statt."
          }
        },
        "dailyCronjobSwitchActive": {
          "type": "checkbox",
          "label": "Pop-up Texte für jeden Tag um 0:00 Uhr automatisch erstellen",
          "tooltip": {
            "type": "info",
            "text": "Tickets für Pop-up Texte werden in das System eingetragen"
          }
